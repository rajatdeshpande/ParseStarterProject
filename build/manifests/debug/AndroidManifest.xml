<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.parse.starter">

    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="19"/>

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:name="com.parse.starter.ParseApplication" android:theme="@android:style/Theme.Holo.Light.DarkActionBar">
        <activity android:label="@string/app_name" android:name="com.parse.starter.ParseStarterProjectActivity">
        </activity>
        <activity android:label="LoanCentral" android:name="com.parse.starter.FoodieToh">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.parse.starter.FindMyRateActivity"/>
        <activity android:name="com.parse.starter.EducationLoanInfoActivity"/>
        <activity android:name="com.parse.starter.HomeLoanInfoActivity"/>
        <activity android:label="@string/title_activity_login" android:name="com.parse.starter.LoginActivity" android:windowSoftInputMode="adjustResize|stateVisible">
        </activity>
        <activity android:label="Checkout" android:name="com.parse.starter.CheckoutActivity"/>
    </application>

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_PROFILE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>

</manifest>
